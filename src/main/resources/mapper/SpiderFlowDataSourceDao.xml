<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd" >


<mapper namespace="com.stonedt.spider.dao.SpiderFlowDataSourceDao">

    <select id="selectListAll" resultType="com.stonedt.spider.entity.SpiderFlowDataSource">
        select id,name,driver_class_name as driverClassName,jdbc_url as jdbcUrl,username,password
        from
                spider-flow.sp_datasource
        order by create_date desc
    </select>
    
    
    
    
    
    
    
    
    
    
    

</mapper>